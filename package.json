    "del": "^5.1.0",
    "gulp-sourcemaps": "^2.6.5",
    "requirejs": "^2.3.6",
    "text": "requirejs/text#latest",
    "gulp-typescript": "^6.0.0-alpha.1",
    "gulp-tslint": "^8.1.4",
    "tslint": "^6.0.0",
    "typescript": "^3.8.2",
    "@types/node": "^13.7.6",
    "gulp-dart-sass": "^1.0.0",
    "gulp-htmlmin": "^5.0.1",
    "browser-sync": "^2.26.7",
    "connect-history-api-fallback": "^1.6.0",
    "debounce": "^1.2.0",
